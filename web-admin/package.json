{
  "name": "web-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{js,vue}\" --cache",
    "lint:fix": "eslint \"**/*.{js,vue}\" --fix --cache",
    "lint:stats": "eslint \"**/*.{js,vue}\" --stats --format json",
    "lint:timing": "TIMING=1 eslint \"**/*.{js,vue}\"",
    "ci:lint": "eslint \"**/*.{js,vue}\" --max-warnings=0 --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prestart:pm2": "mkdir -p logs && npm run build",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "restart:pm2": "pm2 restart ecosystem.config.cjs",
    "stop:pm2": "pm2 stop ecosystem.config.cjs",
    "delete:pm2": "pm2 delete ecosystem.config.cjs",
    "deploy:prod": "npm install && npm run build && pm2 delete ecosystem.config.cjs 2>/dev/null || true && pm2 start ecosystem.config.cjs",
    "redeploy:prod": "npm install && npm run build && pm2 restart ecosystem.config.cjs",
    "logs": "pm2 logs web-admin-preview",
    "status": "pm2 status"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.6",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@eslint/js": "^9.36.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "less": "^4.2.0",
    "pm2": "^5.3.0",
    "prettier": "^3.6.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^7.1.2"
  }
}
